<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:m="urn:import:gwt.material.design.client.ui" >
		
	<m:MaterialPanel marginBottom="5">
		<m:MaterialRow marginBottom="5">
			<m:MaterialColumn grid="l8 m8 s8">
				<m:MaterialCheckBox ui:field="chkToggleUI" text="Gram Const"/>
			</m:MaterialColumn>
			
			<m:MaterialColumn grid="l2 m2 s2" paddingTop="2">
				<m:MaterialIcon ui:field="icoHelpTextUI" iconType="INFO" iconSize="TINY" textColor="GREY"/>
			</m:MaterialColumn>
			
			<m:MaterialColumn grid="l2 m2 s2" paddingTop="2">
				<m:MaterialBadge ui:field="bdgResultCountUI" right="45"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRange ui:field="sldWeightUI" paddingBottom="5"/>
	</m:MaterialPanel>
</ui:UiBinder> 
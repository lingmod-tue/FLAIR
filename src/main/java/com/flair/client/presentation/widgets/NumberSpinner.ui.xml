<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
	<ui:style>
	</ui:style>

	<g:AbsolutePanel ui:field="pnlAbsolutePanel"
		width="55px" height="23px">
		<g:at left='0' top='4'>
			<g:IntegerBox width="30px" height="16px"
				ui:field="boxInteger"></g:IntegerBox>
		</g:at>
		<g:at left='34' top='0'>
			<m:MaterialIcon ui:field="btnUp"
				iconType="ARROW_DROP_UP" iconSize="TINY" iconPosition="LEFT"
				margin="0" />
		</g:at>
		<g:at left='34' top='10'>
			<m:MaterialIcon ui:field="btnDown"
				iconType="ARROW_DROP_DOWN" iconSize="TINY" iconPosition="LEFT"
				margin="0" />
		</g:at>
	</g:AbsolutePanel>

</ui:UiBinder>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.brand-logo {
		padding: 1px 1px 1px 1px;
		text-align: center;
	}
	
	.text-orange {
		color: darkorange;
		font-weight: bold;
	}
	
	.width-adjusted {
		display: inline-block;
	}
	
	.centered {
		text-align: center;
	}
	
	.indent-reversed {
		margin-left: -10px;
	}
	
	.spaced-text {
		padding: 0px;
		margin-right: 5px;
	}
	
	.indent {
		margin-left: 20px;
	}
	</ui:style>

	<m:MaterialContainer>
		<m:MaterialDialog ui:field="mdlAboutUI"
			dismissible="true">
			<m:MaterialDialogHeader>
				<m:MaterialRow addStyleNames="{style.brand-logo}">
					<m:MaterialImage url="img/logo_2.png"
						caption="About FLAIR" height="200px" />
				</m:MaterialRow>
				<g:FlowPanel addStyleNames="{style.centered}">
					<m:MaterialLabel text="by"
						addStyleNames="{style.text-orange},{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink
						href="https://scholar.google.de/citations?user=L_voVf4AAAAJ&amp;hl=en"
						target="_blank" text="Maria Chinkina"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLink iconType="MAIL_OUTLINE"
						textColor="BLUE_LIGHTEN_1" iconSize="TINY"
						href="mailto:mchnkina@sfs.uni-tuebingen.de?Subject=FLAIR%20tool"
						target="_top" addStyleNames="{style.width-adjusted}"
						title="write to Maria Chinkina" />
					<m:MaterialLabel text="&amp;"
						addStyleNames="{style.width-adjusted},{style.indent-reversed},{style.spaced-text}" />
					<m:MaterialLink
						href="http://sfs.uni-tuebingen.de/~mkannan/" target="_blank"
						text="Madeeswaran Kannan" addStyleNames="{style.width-adjusted}" />
					<m:MaterialLink iconType="MAIL_OUTLINE"
						textColor="BLUE_LIGHTEN_1" iconSize="TINY"
						href="mailto:mkannan@sfs.uni-tuebingen.de?Subject=FLAIR%20tool"
						target="_top" addStyleNames="{style.width-adjusted}"
						title="write to Madeeswaran Kannan" />
					<m:MaterialLabel text=", supervised by"
						addStyleNames="{style.width-adjusted},{style.indent-reversed},{style.spaced-text}" />
					<m:MaterialLink
						href="http://sfs.uni-tuebingen.de/~dm/" target="_blank"
						text="Prof. Dr. Detmar Meurers"
						addStyleNames="{style.width-adjusted}" />
				</g:FlowPanel>
				<g:FlowPanel addStyleNames="{style.centered}">
					<m:MaterialLabel text="@"
						addStyleNames="{style.text-orange},{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel
						text="University of Tübingen, Germany"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text="|"
						addStyleNames="{style.text-orange},{style.width-adjusted}" />
					<m:MaterialLabel text="2015-2020"
						addStyleNames="{style.width-adjusted}" />
				</g:FlowPanel>
				<g:FlowPanel addStyleNames="{style.centered}">
					<m:MaterialLabel text="Version 2.1"
						addStyleNames="{style.width-adjusted}" />
				</g:FlowPanel>
			</m:MaterialDialogHeader>
			<m:MaterialDialogContent>
				<m:MaterialRow marginBottom="5">
					<m:MaterialLabel ui:field="lblDescIntro"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel text=":"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel text="●"
						addStyleNames="{style.indent},{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel ui:field="lblSearchBullet"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel text="●"
						addStyleNames="{style.indent},{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel ui:field="lblAnaylzeBullet"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel text="●"
						addStyleNames="{style.indent},{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel ui:field="lblRerankBullet"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<!-- <m:MaterialRow margin="0" padding="0"> <m:MaterialLabel text="● 
					generates factual questions based on the text in the results" addStyleNames="{style.indent}"/> 
					</m:MaterialRow> -->

				<m:MaterialRow margin="0" padding="0" marginTop="40"
					marginBottom="10">
					<m:MaterialLabel ui:field="lblPapers"
						fontWeight="BOLD" addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=":" fontWeight="BOLD"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLink ui:field="lblThesis"
						href="http://sfs.uni-tuebingen.de/~mchnkina/downloads/Chinkina_Maria_thesis_2015.pdf"
						target="_blank"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel text="(Maria Chinkina)"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLink ui:field="lblBEA"
						href="https://aclanthology.org/W16-0521.pdf" target="_blank"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel
						text="(Maria Chinkina, Detmar Meurers)"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLink ui:field="lblSystem"
						href="https://aclanthology.org/P16-4002/" target="_blank"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel
						text="(Maria Chinkina, Madeeswaran Kannan, Detmar Meurers)"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0" marginTop="40"
					marginBottom="10">
					<m:MaterialLabel ui:field="lblAdditional"
						fontWeight="BOLD" addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=":" fontWeight="BOLD"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLink ui:field="lblAWL"
						href="https://www.wgtn.ac.nz/lals/resources/academicwordlist/publications/AWLmostfreqsublists.pdf"
						target="_blank"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLabel text="("
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLink
						href="https://www.victoria.ac.nz/__data/assets/pdf_file/0020/1626131/Coxhead-2000.pdf"
						target="_blank" text="Coxhead, 2000"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=")"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0" marginTop="40"
					marginBottom="10">
					<m:MaterialLabel ui:field="lblThirdParty"
						fontWeight="BOLD" addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=":" fontWeight="BOLD"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel text="Back-end:"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink
						href="https://www.microsoft.com/en-us/bing/apis/bing-web-search-api"
						target="_blank" text="Microsoft Bing API"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=","
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink href="https://tika.apache.org/"
						target="_blank" text="Apache Tika"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=","
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink
						href="http://stanfordnlp.github.io/CoreNLP/" target="_blank"
						text="Stanford CoreNLP" addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel text="Front-end"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink href="http://www.gwtproject.org/"
						target="_blank" text="Google Web Toolkit"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=","
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink
						href="https://gwtmaterialdesign.github.io/gwt-material-demo/"
						target="_blank" text="GWT Material"
						addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=","
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink href="https://d3js.org"
						target="_blank" text="d3" addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0" marginTop="40"
					marginBottom="10">
					<m:MaterialLabel ui:field="lblLicense"
						fontWeight="BOLD" addStyleNames="{style.width-adjusted}" />
					<m:MaterialLabel text=":" fontWeight="BOLD"
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLink
						href="https://www.gnu.org/licenses/gpl-3.0.en.html"
						target="_blank">
						<m:MaterialImage
							url="https://www.gnu.org/graphics/gplv3-127x51.png"
							caption="GPLv3"></m:MaterialImage>
					</m:MaterialLink>
				</m:MaterialRow>
				<m:MaterialRow margin="0" padding="0">
					<m:MaterialLabel ui:field="lblLicenseStmt"
						addStyleNames="{style.width-adjusted},{style.spaced-text}" />
					<m:MaterialLink
						href="https://www.gnu.org/licenses/gpl-3.0.en.html"
						text="GNU General Public License 3.0" target="_blank" />
					<m:MaterialLabel text="."
						addStyleNames="{style.width-adjusted}" />
				</m:MaterialRow>
			</m:MaterialDialogContent>

			<m:MaterialDialogFooter>
				<m:MaterialButton ui:field="btnAboutCloseUI" type="FLAT" />
			</m:MaterialDialogFooter>
		</m:MaterialDialog>

	</m:MaterialContainer>
</ui:UiBinder> 